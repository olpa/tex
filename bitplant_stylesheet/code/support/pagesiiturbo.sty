\newlength\paperfmt@delta@w
\newlength\paperfmt@delta@h
\newlength\paperfmt@delta@x
\newlength\paperfmt@delta@y

\newcommand\AnsiToAmi@Mkbox[6]{%
\ifdim0pt=\paperfmt@delta@w\relax
  \def\b@atoa@neww{#2}%
\else
  \advance\paperfmt@delta@w by #2\relax
  \edef\b@atoa@neww{\the\paperfmt@delta@w}%
  \setlength\paperfmt@delta@w{0pt}%
\fi
\ifdim0pt=\paperfmt@delta@h\relax
  \def\b@atoa@newh{#3}%
\else
  \advance\paperfmt@delta@h by #3\relax
  \edef\b@atoa@newh{\the\paperfmt@delta@h}%
  \setlength\paperfmt@delta@h{0pt}%
\fi
\ifdim0pt=\paperfmt@delta@x\relax
  \def\b@atoa@newx{#4}%
\else
  \advance\paperfmt@delta@x by #4\relax
  \edef\b@atoa@newx{\the\paperfmt@delta@x}%
  \setlength\paperfmt@delta@x{0pt}%
\fi
\ifdim0pt=\paperfmt@delta@y\relax
  \def\b@atoa@newy{#5}%
\else
  \advance\paperfmt@delta@y by #5\relax
  \edef\b@atoa@newy{\the\paperfmt@delta@y}%
  \setlength\paperfmt@delta@y{0pt}%
\fi
\b@orig@td@mkbox{#1}{\b@atoa@neww}{\b@atoa@newh}{\b@atoa@newx}{\b@atoa@newy}{#6}%
}

\newcommand\atoa@correct@h[2]{%
