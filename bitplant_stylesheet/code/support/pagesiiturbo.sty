\def\AnsiToAmiVHSize@ii#1\vsize=#2mm#3\evensidemargin=#4mm#5rewritewith:#6#7#8{%
\def#6{#1\vsize=#7 #3\evensidemargin=#8 #5}}

\newcommand\AIV@ToAmiVHSize[1]{%
